{
  "name": "AssAssembla",
  "version": "0.1.2",
  "description": "Add some simple but nifty features to Assembla",
  "homepage_url": "http://github.com/apwit/assassembla",
  "options_page": "html/options.html",
	"icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.assembla.com/*"],
      "js": ["js/jquery-1.5.2.min.js", "js/feature_factory.js", "js/features.js", "js/content.js"],
      "run_at": "document_start"
    }
  ]
}
